{
  "extends": ["plugin:vue/vue3-recommended", "eslint:recommended"],
  "plugins": [],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": [
    "dist/",
    "node_modules/"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["src/*.spec.js"],
      "rules": {}
    }
  ]
}