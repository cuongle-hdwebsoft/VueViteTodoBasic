{
  "extends": ["plugin:vue/vue3-recommended","prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", 
      {
        "endOfLine": "auto"
      }
    ]
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": [
    "dist/"
  ],
  "parserOptions": {
    "sourceType": "module"
  }
}